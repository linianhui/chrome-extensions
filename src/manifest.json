{
    "manifest_version": 2,
    "name": "Table Of Contents",
    "version": "0.1",
    "description": "Use h1,h2,h3,h4,h5,h6 element build table of contents.",
    "homepage_url": "https://github.com/linianhui/chrome.extensions.header",
    "icons": {
        "16": "icon/icon.16.png",
        "48": "icon/icon.48.png",
        "128": "icon/icon.128.png"
    },
    "permissions": ["activeTab"],
    "background": {
        "scripts": ["js/event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Table Of Contents Toggle"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/table-of-contents.js"],
        "css": ["css/table-of-contents.css"],
        "run_at": "document_end"
    }]
}